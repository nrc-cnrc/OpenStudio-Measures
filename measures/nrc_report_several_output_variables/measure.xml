  <class_name>NrcReportSeveralOutputVariables</class_name>
  <display_name>Nrc Report Several Output Variables</display_name>
  <description>This measure displays csv files for output variables.</description>
  <modeler_description>The measure creates CSV files for output variables entered by the user. The output variables have to be entered in the format :
    OutputVariable1 : Key Name1,OutputVariable2 : Key Name2,OutputVariable3 : Key Name3,...etc
    Also the measures creates hourly data for meter outputs ("Electricity:Facility", "Gas:Facility", "NaturalGas:Facility")</modeler_description>
  <arguments>
    <argument>
      <name>reporting_frequency</name>
      <display_name>Reporting Frequency</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>Hourly</default_value>
      <choices>
        <choice>
          <value>Hourly</value>
          <display_name>Hourly</display_name>
        </choice>
        <choice>
          <value>Timestep</value>
          <display_name>Timestep</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>output_variables</name>
      <display_name>Please Enter the Output Variables in the format 'OutputVariable1 : Key Name1,OutputVariable2 : Key Name2,OutputVariable3 : Key Name3'   </display_name>
      <type>String</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>Heating Coil Heating Rate:*,Baseboard Total Heating Rate:*</default_value>
    </argument>
  </arguments>
