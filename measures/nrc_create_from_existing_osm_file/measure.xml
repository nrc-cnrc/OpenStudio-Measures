  <class_name>NrcCreateFromExistingOsmFile</class_name>
  <display_name>NRC Create From Existing Osm File</display_name>
  <description>The measure searches a folder for a user defined osm file name and updates version of code</description>
  <modeler_description>The measure searches a folder (input_osm_files) in the measure folder for a user defined osm file name.
            There's a Boolean option to update version of code, If the Bool is true then user can select one of 4 options of the code version. Options are: NECB 2011, 2015, 2017 and 2020</modeler_description>
  <arguments>
    <argument>
      <name>upload_osm_file</name>
      <display_name>Upload OSM File</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>EmptyModel.osm</default_value>
      <choices>
        <choice>
          <value>EmptyModel.osm</value>
          <display_name>EmptyModel.osm</display_name>
        </choice>
        <choice>
          <value>smallOffice_Victoria.osm</value>
          <display_name>smallOffice_Victoria.osm</display_name>
        </choice>
        <choice>
          <value>smallOffice_Windsor.osm</value>
          <display_name>smallOffice_Windsor.osm</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>update_code_version</name>
      <display_name>Update to match version of code?</display_name>
      <type>Boolean</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>true</default_value>
      <choices>
        <choice>
          <value>true</value>
          <display_name>true</display_name>
        </choice>
        <choice>
          <value>false</value>
          <display_name>false</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>template</name>
      <display_name>template</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>NECB2020</default_value>
      <choices>
        <choice>
          <value>NECB2011</value>
          <display_name>NECB2011</display_name>
        </choice>
        <choice>
          <value>NECB2015</value>
          <display_name>NECB2015</display_name>
        </choice>
        <choice>
          <value>NECB2017</value>
          <display_name>NECB2017</display_name>
        </choice>
        <choice>
          <value>NECB2020</value>
          <display_name>NECB2020</display_name>
        </choice>
      </choices>
    </argument>
  </arguments>
