  <class_name>NrcSetPumpEfficiency</class_name>
  <display_name>Set Pump Efficiency </display_name>
  <description>Set pump motor efficiency to specified value in all fans in the model (see modeler description for specific object types).</description>
  <modeler_description>The measure sets motor efficiency of constant and variable speed pumps in the model.</modeler_description>
  <arguments>
    <argument>
      <name>pump_eff</name>
      <display_name>Set pump efficiency (fraction between 0.0 and 1.0)</display_name>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>0.91</default_value>
    </argument>
